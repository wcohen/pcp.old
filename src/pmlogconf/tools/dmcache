#pmlogconf-setup 2.0
ident	metrics used by the pcp-dmcache(1) command
probe	dmcache.cache.used values ? include : exclude
	disk.dm.read
	disk.dm.write
	dmcache.cache.used
	dmcache.cache.total
	dmcache.metadata.used
	dmcache.metadata.total
	dmcache.read_hits
	dmcache.read_misses
	dmcache.write_hits
	dmcache.write_misses
