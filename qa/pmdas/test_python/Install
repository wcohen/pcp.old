. $PCP_SHARE_DIR/lib/pmdaproc.sh

iam=test_python
python_opt=true
daemon_opt=false
# set this in the env to force a new pmcd instance
forced_restart=${PCPQA_RESTART_PMCD-false}

pmdaSetup
pmdaInstall
