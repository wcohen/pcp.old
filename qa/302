#!/bin/sh
# PCP QA Test No. 302
# merge two archives, and check that mark records are placed in the
# correct positions!!!
#
# Copyright (c) 1995-2002 Silicon Graphics, Inc.  All Rights Reserved.
#

seq=`basename $0`
echo "QA output created by $seq"

# get standard filters
. ./common.product
. ./common.filter
. ./common.check

status=1	# failure is the default!
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

# deal with an annoying libc error ... using TZ=:Australia/Melbourne
# causes the -z option to report the wrong time, at least for some
# times of the year
#
TZ=EST-10
export TZ

# real QA test starts here

rm -rf $tmp.arch*
pmlogextract -z archives/arch_a archives/arch_b $tmp.arch

# sed is strange to avoid printf issue between platforms
#
pmdumplog -dilmst -z $tmp.arch \
| sed \
    -e 's/14:46:52.146/14:46:52.145/' \
    -e '/\[[0-9][0-9]* bytes\]/d'

# success, all done
status=0
exit
